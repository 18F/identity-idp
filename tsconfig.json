{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "baseUrl": ".",
    "paths": {
      "*": ["typings/*"]
    }
  },
  "include": [
    "app/components",
    "app/javascript/packages",
    "app/javascript/packs",
    "spec/javascripts/spec_helper.d.ts"
  ],
  "typedocOptions": {
    "out": "public/typedoc"
    "entryPointStrategy": "expand",
    "entryPoints": [
      "app/javascript/app/components/index.js",
      "app/javascript/app/utils/index.js",
      "app/javascript/packages/analytics/index.js",
      "app/javascript/packages/build-sass/index.js",
      "app/javascript/packages/clipboard-button/index.js",
      "app/javascript/packages/components/index.js",
      "app/javascript/packages/compose-components/index.js",
      "app/javascript/packages/device/index.js",
      "app/javascript/packages/document-capture-polling/index.js",
      "app/javascript/packages/document-capture/context/index.js",
      "app/javascript/packages/document-capture/index.js",
      "app/javascript/packages/es5-safe/index.js",
      "app/javascript/packages/eslint-plugin/configs/index.js",
      "app/javascript/packages/eslint-plugin/index.js",
      "app/javascript/packages/i18n/index.ts",
      "app/javascript/packages/masked-text-toggle/index.js",
      "app/javascript/packages/normalize-yaml/index.js",
      "app/javascript/packages/normalize-yaml/visitors/index.js",
      "app/javascript/packages/one-time-code-input/index.js",
      "app/javascript/packages/page-data/index.js",
      "app/javascript/packages/personal-key-input/index.js",
      "app/javascript/packages/phone-input/index.js",
      "app/javascript/packages/polyfill/index.js",
      "app/javascript/packages/react-i18n/index.js",
      "app/javascript/packages/step-indicator/index.js",
      "app/javascript/packages/stylelint-config/index.js",
      "app/javascript/packages/test-helpers/index.ts",
      "app/javascript/packages/time-element/index.ts",
      "app/javascript/packages/url/index.ts",
      "app/javascript/packages/validated-field/index.js",
    ]
  },
  "exclude": [
    "**/fixtures",
    "**/*.spec.js",
    "app/javascript/packs/application.js",
    "app/javascript/packs/personal-key-page-controller.js",
    "app/javascript/packs/pw-strength.js",
    "app/javascript/packs/reactivate-account-modal.js",
    "app/javascript/packs/saml-post.js",
    "app/javascript/packs/ssn-field.js",
    "app/javascript/packs/upload-step.js",
    "app/javascript/packs/webauthn-authenticate.js",
    "app/javascript/packs/webauthn-setup.js",
    "app/javascript/packs/webauthn-unhide.js"
  ]
}
