{
  "name": "@18f/identity-idp",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "app/javascript/packages/*"
  ],
  "scripts": {
    "postinstall": "yarn workspace @18f/identity-analytics run postinstall",
    "typecheck": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.cjs,.mjs",
    "lint:css": "stylelint 'app/assets/stylesheets/**/*.scss' 'app/javascript/**/*.scss' 'app/components/*.scss'",
    "lint:openapi": "./scripts/lint-openapi-spec.mjs",
    "test": "mocha 'spec/javascript/**/**spec.+(j|t)s?(x)' 'app/javascript/packages/**/*.spec.+(j|t)s?(x)'",
    "normalize-yaml": "normalize-yaml",
    "generate-browsers-json": "./scripts/generate-browsers-json.js",
    "clean": "rm -rf public/packs/*",
    "prebuild": "yarn run clean",
    "build": "yarn build:js",
    "build:js": "concurrently 'yarn:webpack' 'make browsers.json'",
    "build:css": "build-sass app/assets/stylesheets/*.css.scss app/components/*.scss --load-path=app/assets/stylesheets --out-dir=app/assets/builds"
  },
  "dependencies": {
    "@18f/identity-design-system": "^9.4.0",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.15.3",
    "babel-loader": "^9.1.0",
    "babel-plugin-polyfill-corejs3": "^0.5.2",
    "browserslist": "^4.22.3",
    "cleave.js": "^1.6.0",
    "concurrently": "^8.2.2",
    "core-js": "^3.21.1",
    "foundation-emails": "^2.3.1",
    "intl-tel-input": "^24.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "source-map-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@redocly/openapi-core": "^1.28.5",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chai": "^4.3.7",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cleave.js": "^1.4.7",
    "@types/dirty-chai": "^2.0.2",
    "@types/grecaptcha": "^3.0.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@types/zxcvbn": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "clipboard-polyfill": "^4.1.1",
    "dirty-chai": "^2.0.1",
    "dom-accessibility-api": "^0.5.14",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jsdom": "^25.0.1",
    "json-schema-to-ts": "^3.1.1",
    "mocha": "^11.1.0",
    "mq-polyfill": "^1.1.8",
    "msw": "^2.6.5",
    "prettier": "^3.1.0",
    "quibble": "^0.9.1",
    "react-test-renderer": "^19.0.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "stylelint": "^16.2.1",
    "svgo": "^3.2.0",
    "swr": "^2.0.0",
    "typescript": "5.2.2",
    "yarn-deduplicate": "^6.0.2"
  }
}
